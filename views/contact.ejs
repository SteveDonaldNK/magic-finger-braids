<body class="">

    <section id="contact-section">
        <!-- <h1>Contact Us</h1> -->
        <div class="row gy-4 main-row">
            <div class="col-12 col-xl-8 col-lg-7 shadow-sm p-5 rounded-4 contact-form">
                <h4 class="mb-4 fw-bold">Send a message</h4>
                <form action="">
                    <div class="row gy-4">
                        <div class="col-12 col-xl-6 col-lg-6 col-md-6">
                            <label for="fName">First name</label>
                            <input type="text" placeholder="Your first name" name="firstName" id="fName">
                        </div>
                        <div class="col-12 col-xl-6 col-lg-6 col-md-6">
                            <label for="lName">Last name</label>
                            <input type="text" placeholder="Your last name" name="lastName" id="LName">
                        </div>
                    </div>
                    <div class="row gy-4">
                        <div class="col-12 col-xl-6 col-lg-6 col-md-6">
                            <label for="email">Email</label>
                            <input type="email" placeholder="Your email" name="email" id="email">
                        </div>
                        <div class="col-12 col-xl-6 col-lg-6 col-md-6">
                            <label for="mobile">Mobile</label>
                            <input type="text" placeholder="Your phone number" name="mobile" id="mobile">
                        </div>
                    </div>
                    <label for="message">Message</label>
                    <textarea placeholder="Your message here..." name="message" id="message" cols="30" rows="10"></textarea>
                    <button class="btn btn-dark px-4 py-2 rounded-0 mt-2">Send</button>
                </form>
            </div>
            <div class="col-12 col-xl-4 col-lg-5">
                <div class="contact-info shadow-sm rounded-4 mb-2">
                    <h3 class="fw-bold mb-3">Contact info</h3>
                    <p>
                        <span>
                            <img loading="lazy" src="assets/location.svg" alt="address">
                        </span>
                        <span>7402 Lake Glen drive Glenn Dale MD 20769</span>
                    </p>
                    <p>
                        <span>
                            <img loading="lazy" src="assets/mail.svg" alt="mail">
                        </span>
                        <span class="email">magicfingerbraids@yahoo.com</span>
                    </p>
                    <p>
                        <span>
                            <img loading="lazy" src="assets/phone.svg" alt="phone">
                        </span>
                        <span>+1 (214) 534-0656</span>
                    </p>
                    <div class="hstack pt-3 w-50 justify-content-between">
