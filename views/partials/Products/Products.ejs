<div class="row m-0 gy-4 ">
    <% products.map(product =>{%>
        <div class="col col-6 col-lg-3 col-md-4 col-sm-6 p-0">
            <div class="card border-0 m-auto" >
                <img src="<%= product.image %>" loading="lazy" class="card-img-top" alt="...">
                <div class="card-body px-0">
                  <p class="card-text fw-semibold mb-0"><%= product.name %></p>
                  <p class="card-text fw-semibold mt-1 text-secondary ">$240 - $340</p>
                </div>
              </div>
        </div>
    <% }) %>
</div>